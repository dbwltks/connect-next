# ===========================================
# 로그 시스템 환경 설정
# ===========================================

# 로그 레벨 (0=DEBUG, 1=INFO, 2=WARN, 3=ERROR, 4=OFF)
LOG_LEVEL=1

# 배치 처리 설정
LOG_BATCH_ENABLED=true
LOG_BATCH_SIZE=50
LOG_BATCH_INTERVAL=10000

# 로그 보관 기간 (일)
LOG_ARCHIVE_DAYS=30

# 실시간 로그 스트림 활성화
LOG_REALTIME_ENABLED=true

# 보안 로그 활성화
LOG_SECURITY_ENABLED=true

# 성능 로그 활성화
LOG_PERFORMANCE_ENABLED=true

# 민감 데이터 마스킹 활성화
LOG_SENSITIVE_MASK=true

# 오류 추적 활성화
LOG_ERROR_TRACKING=true

# ===========================================
# 기존 환경 변수들
# ===========================================

# Supabase 설정
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# 기타 설정
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NODE_ENV=development 